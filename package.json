{
  "name": "vue-lazy",
  "version": "0.1.0-alpha.1",
  "description": "Lazyloading based on Intersection Observer API",
  "main": "dist/vue-lazy.min.js",
  "engines": {
    "node": ">=12.13.0 <13.0.0"
  },
  "scripts": {
    "release": "yarn dev && yarn build",
    "dev": "webpack --config ./config/webpack/webpack.dev.js",
    "build": "webpack --config ./config/webpack/webpack.prod.js",
    "lint": "eslint 'src/**/*.{js,vue}' 'config/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bartdominiak/vue-lazy.git"
  },
  "keywords": [
    "vue",
    "vue-lazy"
  ],
  "author": "smugers@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartdominiak/vue-lazy/issues"
  },
  "homepage": "https://github.com/bartdominiak/vue-lazy#README",
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-loader": "^8.1.0",
    "eslint": "^7.2.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "intersection-observer": "^0.10.0",
    "terser-webpack-plugin": "^3.0.5",
    "vue-loader": "^15.9.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "browserslist": "> 0.25%, not dead"
}
